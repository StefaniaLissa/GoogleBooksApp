<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mynavgraph"
    app:startDestination="@id/bookSearchFragment">

    <fragment
        android:id="@+id/bookSearchFragment"
        android:name="com.example.libreriaapi.views.BookSearchFragment"
        android:label="fragment_booksearch"
        tools:layout="@layout/fragment_booksearch" >
        <action
            android:id="@+id/action_bookSearchFragment_to_bookDetailsFragment"
            app:destination="@id/bookDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/bookDetailsFragment"
        android:name="com.example.libreriaapi.views.BookDetailsFragment"
        android:label="fragment_book_details"
        tools:layout="@layout/fragment_book_details" />


</navigation>